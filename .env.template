# Azure Developer CLI Environment Variables Template
# Copy this file to .azure/<your-env-name>/.env and fill in the values
# IMPORTANT: Never commit .env files with real passwords to version control

# Required: MySQL admin password (min 8 chars, must have upper/lower/number/special)
MYSQL_ADMIN_PASSWORD=

# Optional: Override defaults if needed
MYSQL_ADMIN_USER=mysqladmin
WORDPRESS_DB_NAME=wordpress
SITE_NAME=wpsite
ALLOWED_IP_ADDRESS=

# Container images (optional overrides)
WORDPRESS_IMAGE=wordpress:php8.2-fpm
NGINX_IMAGE=nginx:alpine
